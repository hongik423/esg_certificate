# M-CENTER 랜딩페이지 환경변수 설정
# .env.local 파일을 생성하여 아래 내용을 복사하고 실제 값으로 변경하세요.

# 구글시트 연동 (필수)
NEXT_PUBLIC_GOOGLE_SCRIPT_URL=https://script.google.com/macros/s/YOUR_DEPLOYMENT_ID/exec
NEXT_PUBLIC_GOOGLE_SCRIPT_ID=YOUR_GOOGLE_SCRIPT_ID
NEXT_PUBLIC_GOOGLE_SCRIPT_DEPLOYMENT_ID=YOUR_DEPLOYMENT_ID
NEXT_PUBLIC_GOOGLE_SHEETS_ID=YOUR_GOOGLE_SHEETS_ID
NEXT_PUBLIC_GOOGLE_SHEETS_URL=https://docs.google.com/spreadsheets/d/YOUR_GOOGLE_SHEETS_ID/edit

# EmailJS 설정 (이메일 발송)
NEXT_PUBLIC_EMAILJS_SERVICE_ID=service_YOUR_SERVICE_ID
NEXT_PUBLIC_EMAILJS_PUBLIC_KEY=YOUR_PUBLIC_KEY

# OpenAI API 설정 (AI 진단)
OPENAI_API_KEY=sk-proj-YOUR_OPENAI_API_KEY_HERE

# 기본 설정
NODE_ENV=production
NEXT_PUBLIC_API_BASE_URL=https://your-domain.vercel.app
NEXT_PUBLIC_BASE_URL=https://your-domain.vercel.app

# GitHub Pages 배포 시 (선택사항)
GITHUB_PAGES=false
NEXT_PUBLIC_IS_GITHUB_PAGES=false

# 관리자 설정
ADMIN_EMAIL=your-admin@email.com
JWT_SECRET=your-jwt-secret-key-here

# 기능 활성화/비활성화
NOTIFICATION_ENABLED=true
AUTO_REPLY_ENABLED=true 